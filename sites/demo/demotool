def calculate_outcome(price: float, tax_pct: float) -> dict:
    tax_amount = price * (tax_pct / 100)
    total = price + tax_amount
    result = {
        "price": round(price, 2),
        "tax_pct": round(tax_pct, 2),
        "tax_amount": round(tax_amount, 2),
        "total_price": round(total, 2)
    }
    return result
